<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEE2CA"
    tools:context=".HomeScreen">

  <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">



    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/butcart"
        android:layout_width="72dp"
        android:layout_height="59dp"
        android:layout_marginTop="10dp"
        android:background="#F3EEEE"
        android:scaleType="center"
        android:src="@drawable/shopping_cart_20392"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.954"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027"
        app:rippleColor="#F1ECEC"
        tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck,DuplicateSpeakableTextCheck" />


    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="413dp"
        android:layout_height="190dp"
        android:layout_marginTop="140dp"
        android:orientation="horizontal"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

      <androidx.cardview.widget.CardView
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_margin="10dp"
          android:layout_weight="1"
          android:clickable="true"
          android:focusable="true"
          app:cardCornerRadius="25sp"
          app:cardElevation="5dp"
          app:cardMaxElevation="5dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white">

          <TextView
              android:id="@+id/textView1"
              android:layout_width="110dp"
              android:layout_height="wrap_content"
              android:layout_centerHorizontal="true"
              android:layout_marginTop="10dp"
              android:background="@color/white"
              android:text="SOFA"
              android:textAlignment="center"
              android:textColor="#757474"
              android:textSize="34sp"
              android:textStyle="bold"
              android:transitionName="trans_sofaname" />

          <ImageView
              android:id="@+id/imageView1"
              android:layout_width="138dp"
              android:layout_height="103dp"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="0dp"
              android:background="@color/white"
              android:scaleType="fitCenter"
              android:transitionName="trans_sofa"
              app:srcCompat="@drawable/pngfind_com_white_sofa_png_6425783" />
        </RelativeLayout>
      </androidx.cardview.widget.CardView>

      <androidx.cardview.widget.CardView
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_margin="10dp"
          android:layout_weight="1"
          android:clickable="true"

          android:focusable="true"
          app:cardCornerRadius="25sp"
          app:cardElevation="5dp"
          app:cardMaxElevation="5dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white">


          <TextView
              android:id="@+id/textView2"
              android:layout_width="110dp"
              android:layout_height="wrap_content"
              android:layout_centerHorizontal="true"
              android:layout_marginTop="10dp"
              android:background="@color/white"
              android:text="BED"
              android:textAlignment="center"
              android:textColor="#757474"

              android:textSize="34sp"
              android:textStyle="bold"
              android:transitionName="trans_bedname" />

          <ImageView
              android:id="@+id/imageView2"
              android:layout_width="132dp"
              android:layout_height="79dp"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="15dp"
              android:background="@color/white"
              android:scaleType="centerCrop"
              android:transitionName="trans_bed"
              app:srcCompat="@drawable/bed" />
        </RelativeLayout>
      </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@id/linearLayout"
        android:layout_width="414dp"
        android:layout_height="190dp"
        android:orientation="horizontal"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        tools:ignore="MissingConstraints,NotSibling">

      <androidx.cardview.widget.CardView
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_margin="10dp"
          android:layout_weight="1"
          android:clickable="true"

          android:focusable="true"
          app:cardCornerRadius="25sp"
          app:cardElevation="5dp"
          app:cardMaxElevation="5dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white">

          <TextView
              android:id="@+id/textView5"
              android:layout_width="119dp"
              android:layout_height="63dp"
              android:layout_centerHorizontal="true"
              android:background="@color/white"
              android:text="CHAIR"
              android:textAlignment="center"
              android:textColor="#757474"
              android:textSize="34sp"
              android:textStyle="bold"
              android:transitionName="trans_chairname" />

          <ImageView
              android:id="@+id/imageView5"
              android:layout_width="157dp"
              android:layout_height="102dp"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="0dp"
              android:background="@color/white"
              android:scaleType="fitCenter"
              android:src="@drawable/pngwing_com"
              android:transitionName="trans_chair"
              app:srcCompat="@drawable/pngwing_com" />
        </RelativeLayout>
      </androidx.cardview.widget.CardView>

      <androidx.cardview.widget.CardView
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_margin="10dp"
          android:layout_weight="1"
          android:clickable="true"

          android:focusable="true"
          app:cardCornerRadius="25sp"
          app:cardElevation="5dp"
          app:cardMaxElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white">

          <TextView
              android:id="@+id/textView6"
              android:layout_width="144dp"
              android:layout_height="83dp"
              android:layout_centerHorizontal="true"
              android:layout_marginTop="10dp"
              android:background="@color/white"
              android:text="DECOR"
              android:textAlignment="center"
              android:textColor="#757474"
              android:textSize="34sp"
              android:textStyle="bold"
              android:transitionName="trans_decorname" />

          <ImageView
              android:id="@+id/imageView6"
              android:layout_width="140dp"
              android:layout_height="91dp"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="2dp"
              android:background="@color/white"
              android:scaleType="fitXY"
              android:transitionName="trans_decor"
              app:srcCompat="@drawable/lamp" />
        </RelativeLayout>
      </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="413dp"
        android:layout_height="190dp"
        android:layout_marginTop="2dp"
        android:orientation="horizontal"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout1"
        tools:ignore="MissingConstraints">

      <androidx.cardview.widget.CardView
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_margin="10dp"
          android:layout_weight="1"
          android:clickable="true"

          android:focusable="true"
          app:cardCornerRadius="25sp"
          app:cardElevation="5dp"
          app:cardMaxElevation="5dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white">

          <TextView
              android:id="@+id/textView3"
              android:layout_width="110dp"
              android:layout_height="wrap_content"
              android:layout_centerHorizontal="true"
              android:layout_marginTop="10dp"
              android:background="@color/white"
              android:text="TABLE"
              android:textAlignment="center"
              android:textColor="#757474"
              android:textSize="34sp"
              android:textStyle="bold"
              android:transitionName="trans_tablename" />

          <ImageView
              android:id="@+id/imageView3"
              android:layout_width="138dp"
              android:layout_height="103dp"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="0dp"
              android:background="@color/white"
              android:scaleType="fitCenter"
              android:transitionName="trans_table"
              app:srcCompat="@drawable/coffeetable" />
        </RelativeLayout>
      </androidx.cardview.widget.CardView>

      <androidx.cardview.widget.CardView
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_margin="10dp"
          android:layout_weight="1"
          android:clickable="true"

          android:focusable="true"
          app:cardCornerRadius="25sp"
          app:cardElevation="5dp"
          app:cardMaxElevation="5dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white">

          <TextView
              android:id="@+id/textView4"
              android:layout_width="143dp"
              android:layout_height="93dp"
              android:layout_centerHorizontal="true"
              android:layout_marginTop="10dp"
              android:background="@color/white"
              android:text="RECLINER"
              android:textAlignment="center"
              android:textColor="#757474"
              android:textSize="34sp"
              android:textStyle="bold"
              android:transitionName="trans_recname" />

          <ImageView
              android:id="@+id/imageView4"
              android:layout_width="119dp"
              android:layout_height="80dp"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="15dp"
              android:background="@color/white"
              android:scaleType="fitXY"

              android:transitionName="trans_rec"
              app:srcCompat="@drawable/recliner" />
        </RelativeLayout>
      </androidx.cardview.widget.CardView>
    </LinearLayout>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="184dp"
        android:layout_height="44dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="-5dp"
        android:fontFamily="sans-serif-black"
        android:text="Welcome,"
        android:textColor="#757474"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout1"
        app:layout_constraintEnd_toStartOf="@+id/but_cart"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.207" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-5dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="qwertyui"
        android:textSize="60sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.156"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.0" />

  </androidx.constraintlayout.widget.ConstraintLayout>



</ScrollView>